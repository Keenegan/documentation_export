<thead>
<tr>
  <th>Label</th>
  <th>Type</th>
  <th>Description</th>
  <th>Entité(s) référencée(s)</th>
</tr>
</thead>
<tbody>
{% for field in fields %}
<tr>
  <td>{{ field.label }}</td>
  <td>{{ field.field_type }}</td>
  <td>{{ field.description }}</td>
  <td>
    {% for target in field.settings.handler_settings.target_bundles %}
      {{ target | capitalize }}
      {{ not loop.last ? '-' }}
    {% endfor %}
  </td>
<tr>
  {% endfor %}
</tbody>
